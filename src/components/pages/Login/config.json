{
  "header": {
    "title": {
      "titleText": "Welcome to My Pets",
      "titleSize": "big",
      "subText": "Log in",
      "subSize": "normal",
      "isCentered": true
    },
    "icon": {
      "isCustom": false,
      "iconLabel": "cat",
      "style": {
        "fontSize": "5rem"
      },
      "containerStyle": {
        "display": "table",
        "margin": "2rem auto 0 auto"
      }
    }
  },
  "inputs": {
    "email": {
      "label": "Email",
      "type": "email",
      "control": "email",
      "isRequired": true,
      "isVisible": true
    },
    "password": {
      "label": "Password",
      "type": "password",
      "control": "password",
      "isRequired": true,
      "isVisible": true,
      "minLength": 6
    }
  },
  "loginButton": {
    "type": "submit",
    "color": "success",
    "label": "Log in"
  },
  "goToSignUpButton": {
    "type": "button",
    "color": "danger",
    "label": "You can Sign up"
  }
}
