{
  "header": {
    "titleText": "Sign up",
    "titleSize": "big",
    "isCentered": true
  },
  "inputs": {
    "name": {
      "label": "Name",
      "type": "text",
      "control": "name",
      "isRequired": true,
      "minLength": 3,
      "maxLength": 25,
      "isVisible": true
    },
    "lastName": {
      "label": "Last Name",
      "type": "text",
      "control": "lastName",
      "isRequired": true,
      "minLength": 3,
      "maxLength": 25,
      "isVisible": true
    },
    "userName": {
      "label": "User Name",
      "type": "text",
      "control": "userName",
      "isRequired": true,
      "minLength": 4,
      "maxLength": 10,
      "isVisible": true
    },
    "email": {
      "label": "Email",
      "type": "email",
      "control": "email",
      "isRequired": true,
      "isVisible": true
    },
    "password": {
      "label": "Password",
      "type": "password",
      "control": "password",
      "isRequired": true,
      "minLength": 6,
      "isVisible": true
    },
    "repeatPass": {
      "label": "Repeat Password",
      "type": "password",
      "control": "repeatPass",
      "isRequired": true,
      "minLength": 6,
      "isVisible": true
    }
  },
  "signUpButton": {
    "type": "submit",
    "color": "success",
    "label": "Sign up"
  },
  "goToLoginButton": {
    "type": "button",
    "color": "danger",
    "label": "You can log in with your account"
  }
}