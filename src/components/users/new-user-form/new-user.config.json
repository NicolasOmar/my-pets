{
  "inputs": {
    "name": {
      "label": "Name",
      "type": "text",
      "control": "name",
      "isRequired": true,
      "min": 3,
      "max": 25
    },
    "lastName": {
      "label": "Last Name",
      "type": "text",
      "control": "lastName",
      "isRequired": true,
      "min": 3,
      "max": 25
    },
    "userName": {
      "label": "User Name",
      "type": "text",
      "control": "userName",
      "isRequired": true,
      "min": 4,
      "max": 10
