{
  "name": "my-pets",
  "version": "0.2.3",
  "author": "Nicolás Omar González Passerino",
  "license": "MIT",
  "private": false,
  "homepage": "/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasOmar/my-pets.git"
  },
  "bugs": {
    "url": "https://github.com/NicolasOmar/my-pets/issues"
  },
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:prod": "npm run build && npm run preview",
    "build": "vite build",
    "preview": "vite preview",
    "setup": "npm i && husky install",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --write src",
    "lint-staged": "lint-staged -v",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "sass:css": "node-sass --omit-source-map-url --output-style compressed sass/styles.scss",
    "styles:app": "npm run sass:css -- -o public/styles",
    "styles:storybook": "npm run sass:css -- -o .storybook",
    "build:styles": "npm run styles:app && npm run styles:storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@reduxjs/toolkit": "^1.9.5",
    "crypto-js": "^4.1.1",
    "graphql": "^16.6.0",
    "luxon": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.7",
    "react-router-dom": "^6.12.0",
    "redux": "^4.2.1",
    "validator": "^13.9.0",
    "web-vitals": "^3.3.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/blocks": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/react-vite": "^7.0.18",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "bulma": "^0.9.4",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.18",
    "vite": "^4.3.9"
  },
  "lint-staged": {
    "src/**/*.jsx": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.(js|scss)": [
      "prettier --write"
    ]
  }
}
