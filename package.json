{
  "name": "my-pets",
  "version": "0.2.5",
  "author": "Nicolás Omar González Passerino",
  "license": "MIT",
  "private": false,
  "homepage": "/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasOmar/my-pets.git"
  },
  "bugs": {
    "url": "https://github.com/NicolasOmar/my-pets/issues"
  },
  "type": "module",
  "scripts": {
    "start": "vite",
    "start:prod": "npm run build:app && npm run preview",
    "test": "vitest",
    "test:ci": "npm run test -- --coverage",
    "build:app": "vite build",
    "build:storybook": "storybook build",
    "preview": "vite preview",
    "setup": "npm i && husky install",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier src/**/*.jsx --write",
    "lint-staged": "lint-staged -v",
    "storybook": "storybook dev -p 6006",
    "storybook:cli": "npm run storybook -- --no-open",
    "sass:css": "node-sass --omit-source-map-url --output-style compressed sass/styles.scss",
    "styles:app": "npm run sass:css -- -o public/styles",
    "styles:storybook": "npm run sass:css -- -o .storybook",
    "styles:build": "npm run styles:app && npm run styles:storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.8.4",
    "crypto-js": "^4.1.1",
    "graphql": "^16.8.1",
    "luxon": "^3.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "validator": "^13.11.0",
    "web-vitals": "^3.4.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.4",
    "@storybook/addon-interactions": "^7.4.4",
    "@storybook/addon-links": "^7.4.4",
    "@storybook/blocks": "^7.4.4",
    "@storybook/react": "^7.4.4",
    "@storybook/react-vite": "^7.4.4",
    "@storybook/testing-library": "^0.2.1",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "@vitest/coverage-v8": "^0.34.5",
    "bulma": "^0.9.4",
    "eslint": "^8.50.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.14",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.30",
    "prettier": "^3.0.3",
    "prop-types": "^15.8.1",
    "storybook": "^7.4.4",
    "vite": "^4.4.9",
    "vitest": "^0.34.5"
  },
  "lint-staged": {
    "src/**/*.!(test).jsx": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.test.jsx": [
      "prettier --write"
    ],
    "src/**/*.(js|scss)": [
      "prettier --write"
    ]
  }
}
