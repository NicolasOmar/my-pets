{
  "name": "my-pets",
  "version": "0.0.23",
  "author": "Nicolás Omar González Passerino",
  "license": "MIT",
  "private": false,
  "homepage": "/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasOmar/my-pets.git"
  },
  "bugs": {
    "url": "https://github.com/NicolasOmar/my-pets/issues"
  },
  "scripts": {
    "start": "env-cmd -f ./env/local.env react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "npm test -- --env=jsdom --watchAll=false --collectCoverageFrom=**/*.jsx --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint --fix src/**/*.jsx",
    "prettier": "prettier --write src",
    "update-version": "node scripts/update-version.js"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "axios": "^0.21.1",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.9.7",
    "env-cmd": "^10.1.0",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.21.5",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "prompts": "^2.4.0",
    "prop-types": "^15.7.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.jsx": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.(js|scss)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test -- --env=jsdom && npm run update-version"
    }
  }
}
