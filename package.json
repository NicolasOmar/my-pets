{
  "name": "my-pets",
  "version": "0.2.2",
  "author": "Nicolás Omar González Passerino",
  "license": "MIT",
  "private": false,
  "homepage": "/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasOmar/my-pets.git"
  },
  "bugs": {
    "url": "https://github.com/NicolasOmar/my-pets/issues"
  },
  "scripts": {
    "start": "env-cmd -f ./env/local.env react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false --verbose",
    "test:ci": "npm test -- --coverage",
    "test:diff": "npm run test:ci -- --changedSince=main",
    "eject": "react-scripts eject",
    "lint": "eslint --fix src/**/*.jsx",
    "prettier": "prettier --write src",
    "lint-staged": "lint-staged -v",
    "storybook": "storybook dev",
    "build-storybook": "storybook build",
    "sass:css": "node-sass --omit-source-map-url --output-style compressed sass/styles.scss",
    "styles:app": "npm run sass:css -- -o public/styles",
    "styles:storybook": "npm run sass:css -- -o .storybook",
    "build:styles": "npm run styles:app && npm run styles:storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "crypto-js": "^4.1.1",
    "env-cmd": "^10.1.0",
    "graphql": "^16.6.0",
    "luxon": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "validator": "^13.9.0",
    "web-vitals": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@storybook/addon-actions": "^7.0.11",
    "@storybook/addon-essentials": "^7.0.11",
    "@storybook/addon-interactions": "^7.0.11",
    "@storybook/addon-links": "^7.0.11",
    "@storybook/addon-mdx-gfm": "^7.0.11",
    "@storybook/cli": "^7.0.11",
    "@storybook/node-logger": "^7.0.11",
    "@storybook/preset-create-react-app": "^7.0.11",
    "@storybook/react": "^7.0.11",
    "@storybook/react-webpack5": "^7.0.11",
    "@storybook/testing-library": "^0.1.0",
    "babel-loader": "9.1.2",
    "babel-plugin-named-exports-order": "^0.0.2",
    "bulma": "^0.9.4",
    "eslint": "^8.40.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "node-sass": "^8.0.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.11",
    "storybook-dark-mode": "^3.0.0",
    "webpack": "^5.82.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/index.(jsx)",
      "src/functions/**/*.(js)",
      "src/redux/**/*.(js)"
    ]
  },
  "lint-staged": {
    "src/**/*.jsx": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.(js|scss)": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
