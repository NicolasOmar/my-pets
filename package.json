{
  "name": "my-pets",
  "version": "0.1.18",
  "author": "Nicolás Omar González Passerino",
  "license": "MIT",
  "private": false,
  "homepage": "/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasOmar/my-pets.git"
  },
  "bugs": {
    "url": "https://github.com/NicolasOmar/my-pets/issues"
  },
  "scripts": {
    "start": "env-cmd -f ./env/local.env react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:ci": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint --fix src/**/*.jsx",
    "prettier": "prettier --write src",
    "update-version": "node scripts/update-version.js",
    "storybook": "start-storybook -s ./public/styles -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "crypto-js": "^4.0.0",
    "env-cmd": "^10.1.0",
    "graphql": "^15.5.0",
    "node-sass": "^6.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "redux": "^4.1.0",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.6",
    "babel-loader": "8.1.0",
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.21.5",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "prompts": "^2.4.0",
    "prop-types": "^15.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.(jsx)",
      "src/functions/**/*.(js)",
      "src/redux/**/*.(js)"
    ]
  },
  "lint-staged": {
    "src/**/*.jsx": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.(js|scss)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test && npm run update-version"
    }
  }
}
