{
  "name": "my-pets",
  "version": "0.1.75",
  "author": "Nicolás Omar González Passerino",
  "license": "MIT",
  "private": false,
  "homepage": "/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NicolasOmar/my-pets.git"
  },
  "bugs": {
    "url": "https://github.com/NicolasOmar/my-pets/issues"
  },
  "scripts": {
    "start": "env-cmd -f ./env/local.env react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false --verbose",
    "test:ci": "npm test -- --coverage",
    "test:diff": "npm run test:ci -- --changedSince=main",
    "eject": "react-scripts eject",
    "lint": "eslint --fix src/**/*.jsx",
    "prettier": "prettier --write src",
    "update-version": "node scripts/update-version.js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "sass:css": "node-sass --omit-source-map-url --output-style compressed sass/styles.scss",
    "styles:app": "npm run sass:css -- -o public/styles",
    "styles:storybook": "npm run sass:css -- -o .storybook",
    "build:styles": "npm run styles:app && npm run styles:storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "crypto-js": "^4.0.0",
    "env-cmd": "^10.1.0",
    "graphql": "^15.5.0",
    "luxon": "^3.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.1.0",
    "validator": "^13.7.0",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.18.9",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/node-logger": "^6.5.12",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "babel-loader": "8.1.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "bulma": "^0.9.4",
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "node-sass": "^7.0.3",
    "postcss": "^8.4.16",
    "prettier": "^2.2.1",
    "prompts": "^2.4.0",
    "prop-types": "^15.8.1",
    "webpack": "^5.74.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/index.(jsx)",
      "src/functions/**/*.(js)",
      "src/redux/**/*.(js)"
    ]
  },
  "lint-staged": {
    "src/**/*.jsx": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.(js|scss)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build:styles && git add .storybook/styles.css public/styles/styles.css",
      "pre-push": "npm run build && npm test && npm run update-version"
    }
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
